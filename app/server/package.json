{
  "name": "wodin",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "tsc --project tsconfig.node.json",
    "serve": "node dist/server/server.js ../../config",
    "serve-hot": "node dist/server/server.js --hot-reload=true ../../config",
    "dev": "nodemon src/server/server.ts ../../config",
    "dev-hot": "nodemon src/server/server.ts --hot-reload=true ../../config",
    "test:unit": "vitest -c ./vitest/vitest.unit.config.mjs",
    "test:integration": "vitest -c ./vitest/vitest.integration.config.mjs run",
    "coverage": "vitest -c ./vitest/vitest.unit.config.mjs run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "genversion": "genversion --es6 --semi --double src/version.ts",
    "static-serve": "http-server ./public -p 3000",
    "copy-static-files": "cp ../../config/index.html ./public && cp -r ../../config/files ./public && cp -r ../../config/help ./public",
    "build-static-site": "ts-node --project tsconfig.node.json ./src/static-site-builder/wodinBuilder.ts ../../config ./public ./views && npm run copy-static-files",
    "build-static-site-for-action": "ts-node --project tsconfig.node.json ./src/static-site-builder/wodinBuilder.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/compression": "^1.7.5",
    "@types/docopt": "^0.6.37",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/md5": "^2.3.5",
    "@types/morgan": "^1.9.9",
    "@types/mustache": "^4.2.5",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-istanbul": "^2.1.4",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.14.0",
    "http-server": "^14.1.1",
    "genversion": "^3.2.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "compression": "^1.7.5",
    "docopt": "^0.6.2",
    "express": "^4.21.1",
    "ioredis": "^5.4.1",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "mustache": "^4.2.0",
    "uid": "^2.0.2",
    "vitest": "^2.1.4",
    "zoo-ids": "^2.0.7"
  }
}

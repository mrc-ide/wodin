{
  "name": "wodin",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-basic": "vue-cli-service build --target lib --name basicApp ./src/app/basic.ts --inline-vue --no-clean",
    "build-fit": "vue-cli-service build --target lib --name fitApp ./src/app/fit.ts --inline-vue --no-clean",
    "build-stochastic": "vue-cli-service build --target lib --name stochasticApp ./src/app/stochastic.ts --inline-vue --no-clean",
    "copy-js": "mkdir -p ../server/public/js && find dist/ -type f -name '*.umd.js' -exec cp '{}' ../server/public/js/ ';'",
    "copy-css": "mkdir -p ../server/public/css && find dist/ -type f -name '*.css' -exec cp '{}' ../server/public/css/ ';' && cp node_modules/bootstrap/dist/css/bootstrap.css ../server/public/css",
    "build": "npm run build-basic && npm run build-fit && npm run build-stochastic && npm run copy-js && npm run copy-css",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint",
    "e2e-test": "npx playwright test"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "bootstrap": "^5.1.3",
    "vue": "^3.2.29",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.20.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "^4.5.15",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "axios-mock-adapter": "^1.20.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.4.1",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "node-sass": "^6.0.1",
    "sass-loader": "^10.2.1",
    "typescript": "^4.5.5",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^17.0.0",
    "webpack": "^4.46.0"
  }
}

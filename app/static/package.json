{
    "name": "wodin_static",
    "version": "0.1.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build-wodin": "vue-cli-service build --target lib --name wodin ./src/app/wodin.ts --inline-vue",
        "copy-js": "mkdir -p ../server/public/js && cp dist/wodin.umd.min.js ../server/public/js/",
        "copy-bootstrap": "mkdir -p ../server/public/css && cp node_modules/bootstrap/dist/css/bootstrap.css ../server/public/css",
        "copy-css": "cp dist/wodin.css ../server/public/css/",
        "copy-js-map": "cp dist/wodin.umd.min.js.map ../server/public/js/",
        "copy-fonts": "cp -r dist/fonts/ ../server/public/css/",
        "copy-fonts-serve": "cp -r ../server/public/js/fonts/ ../server/public/",
        "copy-all": "concurrently \"npm run copy-js\" \"npm run copy-bootstrap\" && concurrently \"npm run copy-css \" \"npm run copy-js-map\" \"npm run copy-fonts\"",
        "build": "npm run build-wodin && npm run copy-all",
        "test:unit": "vue-cli-service test:unit",
        "test:dev": "vue-cli-service test:unit --watch",
        "lint": "vue-cli-service lint --no-fix",
        "lint:fix": "vue-cli-service lint",
        "e2e-test": "npx playwright test",
        "serve-app": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/hotReload/vue.config.js vue-cli-service serve ./src/app/wodin.ts",
        "format:check": "prettier --check .",
        "format:write": "prettier --write ."
    },
    "resolutions": {
        "ansi-regex": "5.0.1"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.4.0",
        "@monaco-editor/loader": "^1.3.2",
        "@types/d3-format": "^3.0.1",
        "axios": "^0.26.0",
        "bootstrap": "^5.1.3",
        "color": "^4.2.3",
        "csv-parse": "^5.2.1",
        "d3-format": "^3.1.0",
        "i18next": "^22.4.11",
        "markdown-it": "^13.0.1",
        "markdown-it-mathjax": "^2.0.0",
        "moment": "^2.29.4",
        "monaco-editor": "^0.30.1",
        "plotly.js-basic-dist-min": "^2.14.0",
        "uid": "2.0.0",
        "vue": "^3.2.29",
        "vue-feather": "^2.0.0",
        "vue-router": "^4.1.5",
        "vue3-tags-input": "^1.0.12",
        "vuex": "^4.0.2",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@playwright/test": "1.37.0",
        "@types/bootstrap": "^5.2.6",
        "@types/color": "^3.0.6",
        "@types/jest": "^27.4.0",
        "@types/markdown-it": "^12.2.3",
        "@types/plotly.js-basic-dist-min": "^2.12.1",
        "@types/xlsx": "^0.0.36",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "^5.0.8",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "^4.5.15",
        "@vue/eslint-config-airbnb": "^5.0.2",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@vue/test-utils": "^2.0.0-rc.18",
        "axios-mock-adapter": "^1.20.0",
        "concurrently": "^7.2.2",
        "eslint": "^7.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-vue": "^8.4.1",
        "eslint-plugin-vuejs-accessibility": "^1.1.1",
        "feather-icons": "^4.29.0",
        "file-loader": "^6.2.0",
        "jest-raw-loader": "^1.0.1",
        "monaco-editor-webpack-plugin": "^6.0.0",
        "node-sass": "^6.0.1",
        "prettier": "3.1.0",
        "raw-loader": "^4.0.2",
        "read-pkg": "^7.1.0",
        "renderkid": "^3.0.0",
        "sass-loader": "^10.2.1",
        "typescript": "^4.5.5",
        "vue-jest": "^5.0.0-alpha.10",
        "vue-loader": "^17.0.0",
        "webpack": "^4.46.0",
        "webpack-shell-plugin": "^0.5.0"
    }
}

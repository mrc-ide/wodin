{
  "name": "wodin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run copy-assets && vite",
    "build-with-check": "npm run copy-assets && run-p type-check \"build-only {@}\" -- && npm run copy-dist",
    "build": "npm run copy-assets && vite build && npm run copy-dist",
    "static-build": "npm run copy-assets && VITE_STATIC_BUILD=true vite build --config vite-static.config.ts && npm run copy-dist",
    "copy-assets": "rm -rf ../server/public/* && cp -r ./src/assets/ ../server/public/ && cp ../server/assets/favicon.ico ../server/public/.",
    "copy-dist": "cp -r ./dist/. ../server/public/.",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@monaco-editor/loader": "^1.4.0",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "color": "^4.2.3",
    "csv-parse": "^5.5.6",
    "d3-format": "^3.1.0",
    "i18next": "^23.16.4",
    "markdown-it": "^14.1.0",
    "markdown-it-mathjax": "^2.0.0",
    "moment": "^2.30.1",
    "skadi-chart": "file:~/skadi-chart-old",
    "uid": "^2.0.2",
    "vue": "^3.5.12",
    "vue-feather": "^2.0.0",
    "vue-router": "^4.4.5",
    "vue3-tags-input": "^1.0.12",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/bootstrap": "^5.2.10",
    "@types/color": "^3.0.6",
    "@types/d3-format": "^3.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^18.19.61",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-istanbul": "^2.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.13.0",
    "eslint-plugin-playwright": "^2.0.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass-embedded": "~1.66.1",
    "typescript": "~5.6.0",
    "vite": "^5.4.10",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  }
}
